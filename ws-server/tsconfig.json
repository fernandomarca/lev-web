{
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./",
    "target": "ES5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "server.ts",
    "src/**/*.ts",
    "room/**/*.ts",
  ],
  "exclude": [
    "node_modules"
  ]
}